<?xml version="1.0" encoding="UTF-8"?>

<launch>

	<arg name="description" default="Running Follower With Timeheadway"/>
	
	<arg name="th1" default="0.4"/>
	<arg name="th2" default="1.2"/>
	<arg name="th3" default="1.8"/>	
	<arg name="w1" default="24.5"/>
	<arg name="w2" default="26.0"/>
	<arg name="w3" default="25.5"/>

	<param name="/init_gap" value="30.0"/>
	<param name="/th1" value="$(arg th1)"/>
	<param name="/th2" value="$(arg th2)"/>
	<param name="/th3" value="$(arg th3)"/>
	<param name="/w1" value="$(arg w1)"/>
	<param name="/w2" value="$(arg w2)"/>
	<param name="/w3" value="$(arg w3)"/>
	<param name="description" value="$(arg description)"/>


	<!--group ns="catvehicle"-->
	<node pkg="followerstoperth" type="followerstoperth_node" name="followerstoperth_node" output="screen">	
	</node>	

	<node pkg="velocity_controller" type="velocity_controller_node" name="velocity_controller_node" output="screen">	
		<remap from="/vehicle/vel" to="/vel"/>	
	</node>
	

	<node pkg="transfer_pkg" type="saveparam.py" name="saveparam" output="screen">
	</node>
	

</launch>
